    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".customers.dashboard.ConHomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".customers.basic.BasicActivity" />
        <activity android:name=".customers.advance.AdvanceActivity" />
        <activity android:name=".customers.docs.DocTabActivity" />
        <activity android:name=".my_bs.dashboard.AUploadActivity" />
        <activity android:name=".customers.latest.LatestActivity"></activity>
    </application>

</manifest>